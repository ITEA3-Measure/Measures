        <fields fieldName="startdate" fieldType="u_date"/>
        <fields fieldName="duedate" fieldType="u_date"/>
        <fields fieldName="timeestimate" fieldType="u_long"/>
        <fields fieldName="timespent" fieldType="u_long"/>
