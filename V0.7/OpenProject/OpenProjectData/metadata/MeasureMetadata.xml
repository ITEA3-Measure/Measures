        <fields fieldName="creator" fieldType="u_keyword"/>
        <fields fieldName="responsible" fieldType="u_keyword"/>
        <fields fieldName="assignee" fieldType="u_keyword"/>
        <fields fieldName="created" fieldType="u_date"/>
