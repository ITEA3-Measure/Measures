<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Measure category="GitHub" name="GitHubIssueStat" provider="Softeam" type="DIRECT">
    <description>Provide the number of Open / Closed / Total issues in GitHub Repository.</description>
    <scopeProperties defaultValue="" name="Login" type="STRING"/>
    <scopeProperties defaultValue="" name="AccessTocken" type="PASSWORD"/>
    <scopeProperties defaultValue="" name="Organisation" type="STRING"/>
    <scopeProperties defaultValue="" name="Repository" type="STRING"/>
    <scopeProperties defaultValue="2000-01-01T00:00:00z" name="CollectDataSince" type="STRING"/>
    <unit name="GitHubTrackerData">        
        <fields fieldName="Open" fieldType="u_long"/>
        <fields fieldName="Close" fieldType="u_long"/>
        <fields fieldName="Total" fieldType="u_long"/>
        <fields fieldName="OpenDuration" fieldType="u_long"/>      
    </unit>
    <views>


	    <view name="Issues" default="true">
    		<description>Overage Open Duration</description>
    		<datasource timePeriode="2y"/>
    		<layout width="900" height="450"/>
					<customData></customData>
	    </view>
<view name="Average Open Duration" default="true">
    		<description>Overage Open Duration</description>
    		<datasource timePeriode="2y"/>
    		<layout width="450" height="450"/>
					<customData></customData>
	    </view>
	    
	</views>
</Measure>