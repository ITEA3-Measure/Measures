<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Measure category="GitHub" name="GitHubCommit" provider="Softeam" type="DIRECT">
    <description>Retrive Commits informations ( Author, Message,Date , number of ligne added, number of ligne deleted , number of ligne changed))  form an GitHub repository. All commit from the last exection date of the measure are returned.</description>
    <scopeProperties defaultValue="" name="Login" type="STRING"/>
    <scopeProperties defaultValue="" name="AccessTocken" type="PASSWORD"/>
    <scopeProperties defaultValue="" name="Organisation" type="STRING"/>
    <scopeProperties defaultValue="" name="Repository" type="STRING"/>
    <scopeProperties defaultValue="2000-01-01T00:00:00z" name="CollectDataSince" type="STRING"/>
    <unit name="GitHubTrackerData">
        <fields fieldName="author" fieldType="u_keyword"/>
        <fields fieldName="postDate" fieldType="u_date"/>
        <fields fieldName="commitMessage" fieldType="u_text"/>
        <fields fieldName="LigneAdd" fieldType="u_integer"/>
        <fields fieldName="LigneChanged" fieldType="u_integer"/>
        <fields fieldName="LigneDelete" fieldType="u_integer"/>
    </unit>
    <views>
      	<view name="Commits History" default="true">
    		<description>Commits History</description>
    		<datasource timePeriode="2y"/>
    		<layout width="450" height="450"/>
			<customData></customData>
	    </view> 
	    <view name="Repository Activity" default="true">
    		<description>Repository Activity</description>
    		<datasource timePeriode="60d"/>
    		<layout width="450" height="450"/>
			<customData></customData>
		 </view> 
	     <view name="Commits By Users" default="true">
    		<description>Commits By Users</description>
    		<datasource timePeriode="2y"/>
    		<layout width="450" height="450"/>
				<customData></customData>
		  </view> 
		 <view name="Commits" type="TABLE" default="true" autoRefresh="false">
    		<description>Commits</description>
    		<datasource dataIndex="postDate,author,commitMessage,LigneAdd,LigneChanged,LigneDelete" dateIndex="postDate"  timePeriode="2y"/>
    		<layout width="1380" height="600" fontSize="10" color="#FFFFFF" />
    	</view> 
	</views>
</Measure>
